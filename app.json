{
  "expo": {
    "jsEngine": "hermes",
    "name": "Money On Track",
    "slug": "mot-money-on-track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/icon-512x512.png",
    "scheme": "motreactnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icons/icon-192x192.png",
        "monochromeImage": "./assets/images/icons/icon-192x192.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "app.web.money_on_track_1.twa",
      "minSdkVersion": 21,
      "abi": [
        "arm64-v8a",
        "armeabi-v7a"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icons/icon-96x96.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/icon-512x512.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true
        }
      ],
      "expo-secure-store",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1dedc209-df9b-41bb-99d2-b885cd5deb5a"
      }
    }
  }
}
